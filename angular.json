{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ngx-mime": {
      "root": "libs/ngx-mime",
      "sourceRoot": "libs/ngx-mime/src",
      "projectType": "library",
      "prefix": "nationallibraryofnorway",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ngx-mime/tsconfig.lib.json",
            "project": "libs/ngx-mime/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libs/ngx-mime/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ngx-mime/src/test.ts",
            "tsConfig": "libs/ngx-mime/tsconfig.spec.json",
            "karmaConfig": "libs/ngx-mime/karma.conf.js",
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js",
              "./node_modules/hammerjs/hammer.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ngx-mime/tsconfig.lib.json",
              "libs/ngx-mime/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ngx-mime/**"]
          }
        }
      }
    },
    "demo": {
      "root": "apps/demo/",
      "sourceRoot": "apps/demo/src",
      "projectType": "application",
      "prefix": "nationallibraryofnorway",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": ["apps/demo/src/styles.scss", "apps/demo/src/theme.scss"],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/demo/src/test.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.spec.json",
            "karmaConfig": "apps/demo/karma.conf.js",
            "styles": ["apps/demo/src/styles.scss"],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ],
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/tsconfig.app.json",
              "apps/demo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo/**"]
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/demo-e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/demo-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/demo-e2e/**"]
          }
        }
      }
    },
    "integration": {
      "root": "apps/integration/",
      "sourceRoot": "apps/integration/src",
      "projectType": "application",
      "prefix": "nationallibraryofnorway",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/integration",
            "index": "apps/integration/src/index.html",
            "main": "apps/integration/src/main.ts",
            "polyfills": "apps/integration/src/polyfills.ts",
            "tsConfig": "apps/integration/tsconfig.app.json",
            "assets": [
              "apps/integration/src/favicon.ico",
              "apps/integration/src/assets",
              "apps/integration/src/index-elements.html",
              {
                "glob": "**/*",
                "input": "dist/apps/elements/",
                "output": "elements/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/openseadragon/build/openseadragon/",
                "output": "openseadragon/"
              }
            ],
            "styles": [
              "apps/integration/src/styles.scss",
              "apps/integration/src/theme.scss"
            ],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/integration/src/environments/environment.ts",
                  "with": "apps/integration/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "integration:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "integration:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "integration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/integration/src/test.ts",
            "polyfills": "apps/integration/src/polyfills.ts",
            "tsConfig": "apps/integration/tsconfig.spec.json",
            "karmaConfig": "apps/integration/karma.conf.js",
            "styles": ["apps/integration/src/styles.scss"],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ],
            "assets": [
              "apps/integration/src/favicon.ico",
              "apps/integration/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/integration/tsconfig.app.json",
              "apps/integration/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/integration/**"]
          }
        }
      }
    },
    "integration-e2e": {
      "root": "apps/integration-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/integration-e2e/protractor.conf.js",
            "devServerTarget": "integration:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/integration-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/integration-e2e/**"]
          }
        }
      }
    },
    "elements": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/elements",
      "sourceRoot": "apps/elements/src",
      "prefix": "nationallibraryofnorway",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/elements",
            "index": "apps/elements/src/index.html",
            "main": "apps/elements/src/main.ts",
            "polyfills": "apps/elements/src/polyfills.ts",
            "tsConfig": "apps/elements/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/elements/src/favicon.ico",
              "apps/elements/src/assets"
            ],
            "styles": [
              "apps/elements/src/styles.scss",
              "apps/elements/src/theme.scss"
            ],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/elements/src/environments/environment.ts",
                  "with": "apps/elements/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "elements:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "elements:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "elements:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/elements/tsconfig.app.json",
              "apps/elements/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/elements/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/elements/src/test.ts",
            "polyfills": "apps/elements/src/polyfills.ts",
            "tsConfig": "apps/elements/tsconfig.spec.json",
            "karmaConfig": "apps/elements/karma.conf.js",
            "styles": ["apps/elements/src/styles.scss"],
            "scripts": [
              "./node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
            ],
            "assets": [
              "apps/elements/src/favicon.ico",
              "apps/elements/src/assets"
            ]
          }
        }
      }
    },
    "elements-e2e": {
      "root": "apps/elements-e2e",
      "sourceRoot": "apps/elements-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/elements-e2e/cypress.json",
            "tsConfig": "apps/elements-e2e/tsconfig.e2e.json",
            "devServerTarget": "elements:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "elements:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/elements-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/elements-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false,
      "packageManager": "yarn"
    },
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "ngx-mime",
  "schematics": {
    "@nrwl/schematics:library": {
      "unitTestRunner": "karma",
      "framework": "angular"
    },
    "@nrwl/schematics:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "protractor"
    },
    "@nrwl/schematics:node-application": {
      "framework": "express"
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
